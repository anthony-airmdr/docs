{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "maple",
    "name": "AirMDR",
    "colors": {
      "primary": "#0D9373",
      "light": "#07C983",
      "dark": "#0D9373"
    },
    "favicon": "favicon.svg",
    "navigation": {
      "tabs": [
        {
          "tab": "Guides",
          "groups": [
            {
              "group": "Get Started",
